{
  "name": "react-page-nm-plugins-image",
  "version": "4.0.1",
  "main": "./lib/index.js",
  "module": "./lib-es/index.js",
  "typings": "./lib/index.d.ts",
  "author": "as",
  "license": "GNU Lesser General Public License v3.0",
  "scripts": {
    "build": "npm-run-all --parallel build:lib build:css",
    "build:watch": "npm-run-all --parallel build:lib:watch build:css:watch",
    "build:lib": "tsc -p ./tsconfig.json && tsc -p ./tsconfig-es.json",
    "build:lib:watch": "npm run build:lib -- --watch",
    "build:css": "postcss -c ../../../../postcss.config.js --dir lib/ src/index.css",
    "build:css:watch": "npm run build:css -- -w",
    "docs": "documentation build ./lib/**/*.js --format md --github -o ../../../../docs/api/plugins/image.md",
    "prebuild": "yarn clean",
    "clean": "rimraf \"lib\" && rimraf \"lib-es\""
  },
  "peerDependencies": {
    "@material-ui/core": "*",
    "@material-ui/icons": "*",
    "@material-ui/styles": "*",
    "react": "^16.8.6",
    "react-dom": "^16.8.6"
  },
  "dependencies": {
    "react-page-nm-core": "*",
    "react-page-nm-ui": "^4.0.1"
  },
  "publishConfig": {
    "access": "public"
  },
  "gitHead": "91e05cb022ff640ab4a96c1523dc2f357ca10588"
}
